        "required": true,
        "type": "string",
        "default": "profile"
      },
      "pivotPath": {
        "required": true,
        "type": "string",
        "default": "settings"
      }
    },
    "AgentOptions": {
      "Delay": {
        "required": true,
        "type": "float",
        "default": "10.0"
      },
      "Jitter": {
        "required": true,
        "type": "float",
        "default": "0.1"
      },
      "Eol": {
        "required": false,
